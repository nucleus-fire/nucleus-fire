{
    "comments": {
        "blockComment": [
            "<!--",
            "-->"
        ]
    },
    "brackets": [
        [
            "<",
            ">"
        ],
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ],
        [
            "{%",
            "%}"
        ],
        [
            "{{",
            "}}"
        ]
    ],
    "autoClosingPairs": [
        {
            "open": "<",
            "close": ">",
            "notIn": [
                "string"
            ]
        },
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "\"",
            "close": "\""
        },
        {
            "open": "'",
            "close": "'"
        },
        {
            "open": "<!--",
            "close": "-->"
        },
        {
            "open": "{{",
            "close": "}}"
        },
        {
            "open": "{%",
            "close": "%}"
        }
    ],
    "surroundingPairs": [
        {
            "open": "<",
            "close": ">"
        },
        {
            "open": "{",
            "close": "}"
        },
        {
            "open": "[",
            "close": "]"
        },
        {
            "open": "(",
            "close": ")"
        },
        {
            "open": "\"",
            "close": "\""
        },
        {
            "open": "'",
            "close": "'"
        }
    ],
    "colorizedBracketPairs": [
        [
            "{",
            "}"
        ],
        [
            "[",
            "]"
        ],
        [
            "(",
            ")"
        ]
    ],
    "folding": {
        "markers": {
            "start": "^\\s*<!--\\s*#region\\b.*-->",
            "end": "^\\s*<!--\\s*#endregion\\b.*-->"
        }
    },
    "indentationRules": {
        "increaseIndentPattern": "<(?!\\?|(?:area|base|br|col|frame|hr|html|img|input|keygen|link|menuitem|meta|param|source|track|wbr|n:slot|n:outlet|n:include|n:text|n:image)\\b|[^>]*/>)([-_\\.A-Za-z0-9:]+)(?=\\s|>)\\b[^>]*>(?!.*</\\1>)|<!--(?!.*-->)|\\{[^}\"']*$|{%[^%]*$",
        "decreaseIndentPattern": "^\\s*(<\\/[-_\\.A-Za-z0-9:]+\\b[^>]*>|-->|\\}|%})"
    },
    "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)"
}